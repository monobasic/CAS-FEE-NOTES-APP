<h1>Create new Note</h1>

<form id="form-note-add">
  <div class="form-row">
    <label for="title">Title</label> <input type="text" id="title" name="title" required>
  </div>
  <div class="form-row">
    <label for="description">Description</label>
    <textarea name="" id="description" name="description" cols="30" rows="10" required></textarea>
  </div>
  <div class="form-row">
    <label for="priority">Priority</label>
    <ul id="list-priority" class="priority">
      <li><a href="" class="active"><i class="fa fa-bolt fa-3x" aria-hidden="true"></i></a></li>
      <li><a href="" class="active"><i class="fa fa-bolt fa-3x" aria-hidden="true"></i></a></li>
      <li><a href="" class="active"><i class="fa fa-bolt fa-3x" aria-hidden="true"></i></a></li>
      <li><a href=""><i class="fa fa-bolt fa-3x" aria-hidden="true"></i></a></li>
      <li><a href=""><i class="fa fa-bolt fa-3x" aria-hidden="true"></i></a></li>
    </ul>
    <input type="hidden" id="priority" value="3" required/>
  </div>
  <div class="form-row">
    <label for="due">Finished by</label><input type="text" id="due" class="datepicker" name="due" required>
  </div>
  <div class="btn-group">
    <a href="#home" class="btn btn-secondary">Cancel</a>
    <input type="submit" class="btn btn-primary" value="Save"/>
  </div>
</form>
